{
  "apiVersion": "doslab.io/v1",
  "kind": "VirtualMachine",
  "metadata": {
    "name": "test",
    "labels": {
        "host": "vm.node191"
    } 
  },
  "spec": {
    "nodeName": "vm.node191",
    "lifecycle": {
      "plugDisk": {
        "source": "/var/lib/libvirt/pooltest/disk1/disk1", 
        "target": "vdb",
        "live": true, 
        "config": true, 
        "subdriver": "qcow2"
      }
    }
  }
}